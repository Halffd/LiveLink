+&nbsp;
+export interface Config {
+  streams: Array&lt;{
+    id: number;
+    enabled: boolean;
+    screen: number;
+    sources: Array&lt;{
+      type: string;
+      subtype?: string;
+      name?: string;
+      enabled: boolean;
+      limit: number;
+      priority: number;
+      tags?: string[];
+    }&gt;;
+    sorting: {
+      field: string;
+      order: 'asc' | 'desc';
+    };
+    refresh: number;
+    autoStart: boolean;
+    quality: string;
+    volume: number;
+    windowMaximized: boolean;
+  }&gt;;
+  organizations: string[];
+  favoriteChannels: FavoriteChannels;
+  holodex: {
+    apiKey: string;
+  };
+  twitch: {
+    clientId: string;
+    clientSecret: string;
+    streamersFile: string;
+  };
+  player: PlayerSettings &amp; {
+    screens: StreamConfig[];
+  };
+  mpv: {
+    priority?: string;
+    'gpu-context'?: string;
+  };
+  streamlink: {
+    path?: string;
+    options?: Record&lt;string, string&gt;;
+    http_header?: Record&lt;string, string&gt;;
+  };
+}
+&nbsp;
