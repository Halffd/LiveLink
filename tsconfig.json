{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"target": "ESNext",
		"paths": {
			"$components/*": ["./src/components/*"],
			"$lib/*": ["./src/lib/*"],
			"$routes/*": ["./src/routes/*"]
		}
	},
	"include": [
		".svelte-kit/types/**/$types.d.ts",
		"vite.config.ts",
		"src/**/*.js",
		"src/**/*.ts",
		"src/**/*.svelte"
	],
	"exclude": [
		"src/server/**/*",
		"src/cli/**/*"
	]
}
