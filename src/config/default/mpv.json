{
  "priority": "normal",
  "gpu-context": "auto",
  "vo": "gpu",
  "hwdec": "auto",

  "cache": true,
  "cache-secs": 120,
  "demuxer-max-bytes": "800M",
  "demuxer-max-back-bytes": "200M",
  "network-timeout": 60,
  "stream-buffer-size": "64M",

  "keep-open": true,
  "ontop": false,
  "border": false,

  "osd-level": 1,
  "osd-duration": 1000,
  "osd-font": "sans-serif",
  "osd-font-size": 32,
  "osd-color": "#CCFFFFFF",
  "osd-border-color": "#DD322640"
} 