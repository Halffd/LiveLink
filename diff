+    }
+&nbsp;
+    // Update config
+<span class="cstat-no" title="statement not covered" >    <span class="missing-if-branch" title="if path not taken" >I</span>if (!this.config.mpv) {</span>
+<span class="cstat-no" title="statement not covered" >      this.config.mpv = {};</span>
+    }
