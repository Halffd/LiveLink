  const priorities = [
    { value: 'realtime', label: 'Realtime' },
    { value: 'high', label: 'High' },
    { value: 'above_normal', label: 'Above Normal' },
    { value: 'normal', label: 'Normal' },
    { value: 'below_normal', label: 'Below Normal' },
    { value: 'low', label: 'Low' },
    { value: 'idle', label: 'Idle' }
  ];

  const qualities = [
